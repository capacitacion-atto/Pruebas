body{margin-top:-5px;margin-left:-5px;}
#dmenutop{background-color:#3c8dbc;width:100%;height:70px;}
#dmenutop ul {list-style-type: none;overflow: hidden;background-color: #3c8dbc;font-family:'regular';font-size:11px;padding: 14px 7px 3px;}
#dmenutop li {float: left;list-style:none;}
 li a, .dropbtn {display: inline-block;color: white;text-align: center;padding: 12px 10px;text-decoration: none;}
 li a:hover, .dropdown:hover .dropbtn {background-color: #335a8a;}
 li i:hover, .dropdown:hover .dropbtn {background-color: #335a8a;}
	.dropdown {display: inline-block;}
.dropdown-content {display: none; position: absolute; background-color: #eaf0f9; min-width: 180px; z-index: 1;margin-left:17px;}
.dropdown-content a { color: black;padding: 12px 16px;text-decoration: none;display: block;text-align: left;}
.dropdown-content a:hover {background-color: #000;color:white;}
.dropdown:hover .dropdown-content {display: block;}
.white{color:white;}

#mar1{display:none;}
#mar2{display:none;)}